{
  "name": "jinya/backend",
  "homepage": "https://jinya.de",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-pdo_mysql": "*",
    "laminas/laminas-crypt": "3.3.1",
    "laminas/laminas-db": "2.11.3",
    "laminas/laminas-eventmanager": "3.2.1",
    "laminas/laminas-hydrator": "3.0.2",
    "laminas/laminas-serializer": "2.9.1",
    "league/plates": "3.3.0",
    "monolog/monolog": "1.25.3",
    "php-di/php-di": "6.1.0",
    "phpmailer/phpmailer": "6.1.5",
    "slim/psr7": "0.5.0",
    "slim/slim": "4.5.0",
    "slim/views": "0.1.3",
    "vlucas/phpdotenv": "4.1.4",
    "ext-intl": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "./vendor/bin/phpunit"
  }
}
