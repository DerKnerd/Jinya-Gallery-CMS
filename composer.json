{
    "name": "jinya/jinya-gallery-cms",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Jinya\\": "src/"
        }
    },
    "suggest": {
        "ext-iconv": "*",
        "ext-imagick": "*"
    },
    "require": {
        "php": ">=7.1",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "anahkiasen/underscore-php": "2.0.0",
        "behat/transliterator": "1.2.0",
        "doctrine/doctrine-bundle": "1.11.2",
        "doctrine/migrations": "2.1.1",
        "doctrine/orm": "2.6.3",
        "guzzlehttp/guzzle": "6.3.3",
        "incenteev/composer-parameter-handler": "2.1.3",
        "jinya/profiling-bundle": "0.8.0",
        "leafo/scssphp": "0.8.4",
        "ocramius/proxy-manager": "2.2.3",
        "patchwork/jsqueeze": "2.0.5",
        "psr/event-dispatcher": "1.0.0",
        "sensio/framework-extra-bundle": "5.4.1",
        "symfony/asset": "4.3.4",
        "symfony/console": "4.3.4",
        "symfony/debug-pack": "1.0.7",
        "symfony/dotenv": "4.3.4",
        "symfony/expression-language": "4.3.4",
        "symfony/flex": "1.4.5",
        "symfony/form": "4.3.4",
        "symfony/framework-bundle": "4.3.4",
        "symfony/monolog-bundle": "3.4.0",
        "symfony/orm-pack": "1.0.6",
        "symfony/polyfill-apcu": "1.12.0",
        "symfony/polyfill-iconv": "1.12.0",
        "symfony/polyfill-php73": "1.12.0",
        "symfony/process": "4.3.4",
        "symfony/security-bundle": "4.3.4",
        "symfony/security-csrf": "4.3.4",
        "symfony/serializer-pack": "1.0.2",
        "symfony/swiftmailer-bundle": "3.2.8",
        "symfony/templating": "4.3.4",
        "symfony/translation": "4.3.4",
        "symfony/validator": "4.3.4",
        "symfony/web-link": "4.3.4",
        "symfony/webpack-encore-bundle": "1.6.2",
        "symfony/yaml": "4.3.4",
        "twig/extensions": "1.5.4",
        "twig/twig": "2.11.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.15.3",
        "squizlabs/php_codesniffer": "3.4.2",
        "symfony/browser-kit": "4.3.4",
        "symfony/css-selector": "4.3.4",
        "symfony/maker-bundle": "1.13.0",
        "symfony/profiler-pack": "1.0.4",
        "symfony/web-server-bundle": "4.3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C6AHC4CM8V69S4YAXYKHSZ4H",
            "allow-contrib": false
        }
    }
}
