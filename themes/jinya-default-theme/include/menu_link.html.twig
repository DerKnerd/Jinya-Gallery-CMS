<a class="{{ class }} {% if isActiveMenuItem(item) %}is--current{% endif %} {% if item.highlighted %}is--highlight{% endif %}"
   href="
        {%- if item.route.url is not empty and item.route.url != '/#' -%}
            {{- item.route.url -}}
        {%- endif -%}
"
        {%- if item.pageType == 'external' %} target=" _blank"{%- endif -%}
>
    {{- item.title -}}
</a>
